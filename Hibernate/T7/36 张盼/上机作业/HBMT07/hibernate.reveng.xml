<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="td_dept" schema="dbo" catalog="db_hibernate"
		class="com.qhit.lh.gr3.liu.hb.bean.Dept">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="td_type" schema="dbo" catalog="db_hibernate"
		class="com.qhit.lh.gr3.liu.hb.bean.Type">
		<primary-key>
			<generator class="foreign"></generator>
		</primary-key>
	</table>
<table name="td_user" schema="dbo" catalog="db_hibernate"
	class="com.qhit.lh.gr3.liu.hb.bean.User">
	<primary-key>
		<generator class="native"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>